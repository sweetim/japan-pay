    "pregenerate": "rm abis/* && find ../contract/artifacts/contracts -type f -name \"*.json\" ! -name \"*.dbg.json\" | xargs -I{} cp {} ./abis",
    "generate": "npx wagmi generate"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@tanstack/react-query": "^5.40.1",
    "@web3auth/base": "^8.6.2",
