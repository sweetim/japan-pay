    "@web3auth/modal": "^8.6.2",
    "@web3auth/modal-react-hooks": "^8.6.2",
    "@web3auth/wallet-services-plugin-react-hooks": "^8.6.2",
    "@yudiel/react-qr-scanner": "^2.0.3",
    "antd": "^5.18.0",
    "immer": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "uuid": "^10.0.0",
    "viem": "^2.13.7",
    "wagmi": "^2.9.10",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
