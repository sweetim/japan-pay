    "@vitejs/plugin-react-swc": "^3.5.0",
    "@wagmi/cli": "^2.1.8",
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "empty-module": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
