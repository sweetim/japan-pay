{
  "name": "japan-pay",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "patch:web3auth": "node web3authPatch.js",
    "dev": "vite",
    "prebuild": "npm run patch:web3auth",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
